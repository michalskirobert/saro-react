    <th>Title</th>\n                    <th>Published date</th>\n                    <th>Author</th>\n                    <th>Menagement</th>\n                  </tr>\n                </thead>\n                {newsItems.map((post, index) => {\n                  const { crew, title, date, id, type } = post;\n                  return (\n                    <tbody key={id}>\n                      <tr>\n                        <td>{index}</td>\n                        <td>{title}</td>\n                        <td>{date}</td>\n                        <td>{crew}</td>\n                        <td>\n                          <Button\n                            {...{\n                              variant: \"primary\",\n                              onClick: () => handleEdit(id, type),\n                            }}\n                          >\n                            Edit\n                          </Button>\n                          <Button\n                            {...{\n                              variant: \"danger\",\n                              onClick: () => removeItem(id),\n                            }}\n                          >\n                            Remove\n                          </Button>\n                        </td>\n                      </tr>\n                    </tbody>\n                  );\n                })}\n              </Table>\n            </Tab>\n            <Tab eventKey=\"eventsContent\" title=\"Events menadÅ¼errrooo\">\n              <Table striped bordered hover>\n                <thead>\n                  <tr>\n                    <th>#</th>\n                    <th>Title</th>\n                    <th>Published date</th>\n                    <th>Author</th>\n                    <th>Menagement</th>\n                  </tr>\n                </thead>\n                {newsEvents.map((post, index) => {\n                  const { crew, title, date, id, type } = post;\n                  return (\n                    <tbody key={id}>\n                      <tr>\n                        <td>{index}</td>\n                        <td>{title}</td>\n                        <td>{date}</td>\n                        <td>{crew}</td>\n                        <td>\n                          <Button\n                            {...{\n                              variant: \"primary\",\n                              onClick: () => handleEdit(id, type),\n                            }}\n                          >\n                            Edit\n                          </Button>\n                          <Button\n                            {...{\n                              variant: \"danger\",\n                              onClick: () => removeItem(id, type),\n                            }}\n                          >\n                            Remove\n                          </Button>\n                        </td>\n                      </tr>\n                    </tbody>\n                  );\n                })}\n              </Table>\n            </Tab>\n          </Tabs>\n        </Tab>\n        <Tab eventKey=\"menagmentOfCrew\" title=\"Menage your profile\" disabled>\n          2\n        </Tab>\n        <Tab eventKey=\"menagmentOfPictrues\" title=\"Menage pictrues\" disabled>\n          3\n        </Tab>\n      </Tabs>\n    </section>\n  );\n};\n\nexport default AdminPanel;\n","/Users/kadheryna/saro-react/src/pages/special/edit/AdminTranslate.jsx",[],"/Users/kadheryna/saro-react/src/_reducers/cms.reducer.js",[],"/Users/kadheryna/saro-react/src/components/feature/auth/signup/SignUp.jsx",[],"/Users/kadheryna/saro-react/src/components/feature/auth/login/Login.jsx",["335"],"import React, { useEffect } from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport LoginForm from \"./LoginForm\";\nimport { auth } from \"../../firebase\";\nimport { userActions } from \"../../../../utils/_actions\";\n\nconst Login = () => {\n  const dispatch = useDispatch();\n\n  const logout = async () => {\n    try {\n      dispatch(userActions.logout());\n      await auth.signOut();\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  useEffect(() => {\n    logout();\n  }, []);\n\n  return (\n    <section className=\"section login\">\n      <LoginForm />\n    </section>\n  );\n};\n\nexport default Login;\n","/Users/kadheryna/saro-react/src/components/layout/nav/Nav.jsx",[],"/Users/kadheryna/saro-react/src/pages/special/edit/Edit.jsx",["336"],"import React, { useEffect } from \"react\";\nimport Select from \"react-select\";\nimport { Editor } from \"@tinymce/tinymce-react\";\nimport { Button } from \"react-bootstrap\";\n\nimport CmsAlert from \"./../../../components/shared/alerts/CmsAlert\";\nimport { useEdit } from \"./container\";\n\nconst cities = [\n  {\n    city: \"Warsaw\",\n  },\n  {\n    city: \"Cracow\",\n  },\n];\nconst lang = [\n  {\n    lang: \"en\",\n  },\n  {\n    lang: \"ja\",\n  },\n];\nconst categories = [\n  {\n    id: 1,\n    name: \"Events\",\n  },\n  {\n    id: 2,\n    name: \"Food\",\n  },\n  {\n    id: 3,\n    name: \"Traditions\",\n  },\n];\n\nconst people = [\n  {\n    id: 1,\n    name: \"Robert\",\n  },\n  {\n    id: 22,\n    name: \"xxx\",\n  },\n];\n\nconst Edit = () => {\n  const {\n    alert,\n    handleEdtiorChange,\n    handlerSubmit,\n    getEvent,\n    editableContainer,\n    setEditableContainer,\n  } = useEdit();\n\n  useEffect(() => {\n    getEvent();\n  }, []);\n\n  return (\n    <section className=\"section add-news\">\n      {alert && <CmsAlert />}\n      \n      <form className=\"cms\" onSubmit={handlerSubmit}>\n        <h2 className=\"main-title\">Edit element</h2>\n        <section className=\"form-container\">\n          <div className=\"form-control\">\n            <label htmlFor=\"title\">Title</label>\n            <input\n              id=\"title\"\n              placeholder={editableContainer ? editableContainer.title : \"add title...\"}\n              type=\"text\"\n              value={editableContainer.title}\n              onChange={(e) => {\n              const value = e.target.value;\n              setEditableContainer(prevState => {return {...prevState, title: value}})\n              }}\n            />\n          </div>\n          <div className=\"form-control\">\n            <label htmlFor=\"city\">City</label>\n            <Select\n              {...{\n                id: \"city\",\n                name: \"city\",\n                defaultValue: cities[0],\n                options: cities.map((item) => ({\n                  label: item.city,\n                  value: item.city,\n                })),\n                onChange: (options) => {\n                  setEditableContainer(prevState => {return {...prevState, city: options}})\n                },\n              }}\n            />\n          </div>\n          <div className=\"form-control\">\n            <label htmlFor=\"place\">Place</label>\n            <input\n              id=\"place\"\n              placeholder={editableContainer ? editableContainer.place : \"add place...\"}\n              type=\"text\"\n              value={editableContainer.place}\n              onChange={(e) => {\n                const value = e.target.value;\n              setEditableContainer(prevState => {return {...prevState, place: value}})\n              }}\n            />\n          </div>\n\n          <div className=\"form-control\">\n            <label htmlFor=\"date\">Date</label>\n            <input\n              id=\"date\"\n              type=\"date\"\n              placeholder={editableContainer ? editableContainer.date : null}\n              value={editableContainer.date}\n              onChange={(e) => {\n                const value = e.target.value;\n              setEditableContainer(prevState => {return {...prevState, date: value}})\n              }}\n            />\n          </div>\n          <div className=\"form-control\">\n            <label htmlFor=\"time\">Time</label>\n            <input\n              id=\"time\"\n              type=\"time\"\n              placeholder={editableContainer ? editableContainer.time : null}\n              value={editableContainer.time}\n              onChange={(e) => {\n                const value = e.target.value;\n              setEditableContainer(prevState => {return {...prevState, time: value}})\n              }}\n            />\n          </div>\n          <div className=\"form-control\">\n            <label htmlFor=\"imgURL\">Img URL</label>\n            <input\n              id=\"imgURL\"\n              placeholder={editableContainer ? editableContainer.imgURL : \"add img URL...\"}\n              type=\"text\"\n              value={editableContainer.imgURL}\n              onChange={(e) => {\n                const value = e.target.value;\n              setEditableContainer(prevState => {return {...prevState, imgURL: value}})\n              }}\n            />\n          </div>\n          <div className=\"form-control\">\n            <label htmlFor=\"link\">Link</label>\n            <input\n              id=\"link\"\n              placeholder={editableContainer ? editableContainer.link : \"add link...\"}\n              type=\"text\"\n              value={editableContainer.link}\n              onChange={(e) => {\n                const value = e.target.value;\n                setEditableContainer(prevState => {return {...prevState, link: value}})\n              }}\n            />\n          </div>\n          <div className=\"form-control\">\n            <label htmlFor=\"category\">Category</label>\n            <Select\n              {...{\n                id: \"category\",\n                name: \"category\",\n                defaultValue: categories[0],\n                options: categories.map((item) => ({\n                  label: item.name,\n                  value: item.name,\n                })),\n                onChange: (options) => {\n                  setEditableContainer(prevState => {return {...prevState, category: options}})\n                },\n              }}\n            />\n          </div>\n          <div className=\"form-control\">\n            <label htmlFor=\"lang\">Lang</label>\n            <Select\n              {...{\n                id: \"lang\",\n                name: \"lang\",\n                defaultValue: lang[0],\n                options: lang.map((item) => ({\n                  label: item.lang,\n                  value: item.lang,\n                })),\n                onChange: (options) => {\n                  setEditableContainer(prevState => {return {...prevState, language: options}})},\n              }}\n            />\n          </div>\n          <div className=\"form-control\">\n            <label htmlFor=\"crew\">Crew</label>\n            <Select\n              {...{\n                id: \"crew\",\n                name: \"crew\",\n                defaultValue: people[0],\n                options: people.map((item) => ({\n                  label: item.name,\n                  value: item.name,\n                })),\n                onChange: (options) => {setEditableContainer(prevState => {return {...prevState, crew: options}})},\n              }}\n            />\n          </div>\n        </section>\n\n        <div className=\"form-control form-info\">\n          <label htmlFor=\"content\">Info</label>\n          <textarea\n            id=\"content\"\n            placeholder={editableContainer ? editableContainer.content : \"add description...\"}\n            value={editableContainer.content}\n            onChange={(e) => {\n              const value = e.target.value;\n                setEditableContainer(prevState => {return {...prevState, content: value}})\n            }}\n            cols=\"30\"\n            rows=\"10\"\n          ></textarea>\n        </div>\n        <section className=\"editor\">\n          <Editor\n            apiKey={`${process.env.REACT_APP_TINY_API_KEY}`}\n            initialValue={editableContainer.content