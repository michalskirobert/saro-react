uff\r\n            </Nav.Link>\r\n          </Nav>\r\n          <Table striped bordered hover>\r\n            <thead>\r\n              <tr>\r\n                <th>#</th>\r\n                <th>Title</th>\r\n                <th>Published date</th>\r\n                <th>Author</th>\r\n                <th>Menagement</th>\r\n              </tr>\r\n            </thead>\r\n            {newsItems.map((post, index) => {\r\n              const { author, title, date, id } = post;\r\n              return (\r\n                <tbody key={id}>\r\n                  <tr>\r\n                    <td>{index}</td>\r\n                    <td>{title}</td>\r\n                    <td>{date}</td>\r\n                    <td>{author}</td>\r\n                    <td>\r\n                      <Button variant=\"primary\" disabled>\r\n                        Edit\r\n                      </Button>\r\n                      <Button variant=\"danger\" onClick={() => removeItem(id)}>\r\n                        Remove\r\n                      </Button>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              );\r\n            })}\r\n          </Table>\r\n        </Tab>\r\n        <Tab eventKey=\"menagmentOfCrew\" title=\"Menage your profile\" disabled>\r\n          2\r\n        </Tab>\r\n        <Tab eventKey=\"menagmentOfPictrues\" title=\"Menage pictrues\" disabled>\r\n          3\r\n        </Tab>\r\n      </Tabs>\r\n    </section>\r\n  );\r\n};\r\n\r\nexport default AdminPanel;\r\n","C:\\Users\\jasko\\OneDrive\\Desktop\\Programming\\SARO\\saro-react\\src\\pages\\private\\profile\\Settings.jsx",[],"C:\\Users\\jasko\\OneDrive\\Desktop\\Programming\\SARO\\saro-react\\src\\pages\\special\\add\\AddNews.jsx",[],"C:\\Users\\jasko\\OneDrive\\Desktop\\Programming\\SARO\\saro-react\\src\\pages\\private\\profile\\User.jsx",[],"C:\\Users\\jasko\\OneDrive\\Desktop\\Programming\\SARO\\saro-react\\src\\pages\\public\\home\\Home.jsx",[],"C:\\Users\\jasko\\OneDrive\\De