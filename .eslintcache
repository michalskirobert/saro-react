             <div className=\"content\">\n                <b>{date}</b>\n                <i>{author}</i>\n                <p>{content}</p>\n              </div>\n            </div>\n          </>\n        );\n      })}\n    </article>\n  );\n};\n\nexport default SingleArticle;\n","/Users/kadheryna/saro-react/src/pages/public/about/About.jsx",[],"/Users/kadheryna/saro-react/src/pages/public/about/Profile.jsx",[],"/Users/kadheryna/saro-react/src/pages/public/404/Error.jsx",[],"/Users/kadheryna/saro-react/src/_reducers/cms.reducer.js",[],"/Users/kadheryna/saro-react/src/pages/private/profile/Progress.jsx",[],"/Users/kadheryna/saro-react/src/pages/private/dashboard/Dashboard.jsx",[],"/Users/kadheryna/saro-react/src/pages/private/profile/Settings.jsx",[],"/Users/kadheryna/saro-react/src/pages/private/profile/User.jsx",[],"/Users/kadheryna/saro-react/src/pages/special/add/AddArticle.jsx",[],"/Users/kadheryna/saro-react/src/pages/special/add/AddNews.jsx",[],"/Users/kadheryna/saro-react/src/pages/special/add/AddEvents.jsx",[],"/Users/kadheryna/saro-react/src/pages/special/edit/AdminTranslate.jsx",[],"/Users/kadheryna/saro-react/src/pages/special/edit/Edit.jsx",["337"],"import React, { useEffect } from \"react\";\nimport { Editor } from \"@tinymce/tinymce-react\";\nimport { Button } from \"react-bootstrap\";\n\nimport CmsAlert from \"./../../../components/shared/alerts/CmsAlert\";\nimport { useContainer } from \"./container\";\n\nconst cities = [\n  {\n    city: \"Warsaw\",\n  },\n  {\n    city: \"Cracow\",\n  },\n];\nconst lang = [\n  {\n    lang: \"en\",\n  },\n  {\n    lang: \"ja\",\n  },\n];\nconst categories = [\n  {\n    id: 1,\n    name: \"Events\",\n  },\n  {\n    id: 2,\n    name: \"Food\",\n  },\n  {\n    id: 3,\n    name: \"Traditions\",\n  },\n];\n\nconst people = [\n  {\n    id: 1,\n    name: \"Robert\",\n  },\n  {\n    id: 22,\n    name: \"xxx\",\n  },\n];\n\nconst Edit = () => {\n\n  useEffect(()=>{\n    getEvent()\n  }, [])\n\n  const {\n    handleEdtiorChange,\n    handlerSubmit,\n    getEvent,\n    alert,\n    title, setTitle,\n    imgURL, setImgURL,\n    query,\n    eventDate, setEventDate,\n    setCrew,\n    setEventCity,\n    eventPlace, setEventPlace,\n    link, setLink,\n    eventTime, setEventTime,\n    setLanguage,\n    info, setInfo,\n    setCategory,\n  } = useContainer();\n\n  return (\n    <section className=\"section add-news\">\n      {alert && <CmsAlert />}\n      <form className=\"cms\" onSubmit={handlerSubmit}>\n        <h2 class=\"main-title\">Edit element</h2>\n        <section className=\"form-container\">\n          <div className=\"form-control\">\n            <label htmlFor=\"title\">Title</label>\n            <input\n              id=\"title\"\n              placeholder={title}\n              type=\"text\"\n              value={title}\n              onChange={(e) => setTitle(e.target.value)}\n            />\n          </div>\n          <div className=\"form-control\">\n            <label htmlFor=\"city\">City</label>\n            <select id=\"city\" onChange={(e) => setEventCity(e.target.value)}>\n              {cities &&\n                cities.map((item, index) => {\n                  const { city } = item;\n                  return (\n                    <option key={index} value={city}>\n                      {city}\n                    </option>\n                  );\n                })}\n            </select>\n          </div>\n          <div className=\"form-control\">\n            <label htmlFor=\"place\">Place</label>\n            <input\n              id=\"place\"\n              placeholder={eventPlace}\n              type=\"text\"\n              value={eventPlace}\n              onChange={(e) => {\n                setEventPlace(e.target.value);\n              }}\n            />\n          </div>\n\n          <div className=\"form-control\">\n            <label htmlFor=\"date\">Date</label>\n            <input\n              id=\"date\"\n              type=\"date\"\n              value={eventDate}\n              onChange={(e) => {\n                setEventDate(e.target.value);\n              }}\n            />\n          </div>\n          <div className=\"form-control\">\n            <label htmlFor=\"time\">Time</label>\n            <input\n              id=\"time\"\n              type=\"time\"\n              value={eventTime}\n              onChange={(e) => {\n                setEventTime(e.target.value);\n              }}\n            />\n          </div>\n          <div className=\"form-control\">\n            <label htmlFor=\"imgURL\">Img URL</label>\n            <input\n              id=\"imgURL\"\n              placeholder={imgURL}\n              type=\"text\"\n              value={imgURL}\n              onChange={(e) => {\n                setImgURL(e.target.value);\n              }}\n            />\n          </div>\n          <div className=\"form-control\">\n            <label htmlFor=\"link\">Link</label>\n            <input\n              id=\"link\"\n              placeholder={link}\n              type=\"text\"\n              value={link}\n              onChange={(e) => {\n                setLink(e.target.value);\n              }}\n            />\n          </div>\n          <div className=\"form-control\">\n            <label htmlFor=\"category\">Category</label>\n            <select\n              id=\"category\"\n              onChange={(e) => setCategory(e.target.value)}\n            >\n              {categories.map(({ name, id }) => {\n                return (\n                  <option key={id} value={name}>\n                    {name}\n                  </option>\n                );\n              })}\n            </select>\n          </div>\n          <div className=\"form-control\">\n            <label htmlFor=\"lang\">Lang</label>\n            <select id=\"lang\" onChange={(e) => setLanguage(e.target.value)}>\n              {lang.map((item, index) => {\n                return (\n                  <option key={index} value={item.lang}>\n                    {item.lang}\n                  </option>\n                );\n              })}\n            </select>\n          </div>\n          <div className=\"form-control\">\n            <label htmlFor=\"crew\">Crew</label>\n            <select id=\"crew\" onChange={(e) => setCrew(e.target.value)}>\n              {people.map(({ name, id }) => {\n                return (\n                  <option key={id} value={name}>\n                    {name}\n                  </option>\n                );\n              })}\n            </select>\n          </div>\n        </section>\n\n        <div className=\"form-control form-info\">\n            <label htmlFor=\"info\">Info</label>\n            <textarea\n              id=\"info\"\n              placeholder=\"add event details\"\n              value={info}\n              onChange={(e) => {\n                setInfo(e.target.value);\n              }}\n              cols=\"30\"\n              rows=\"10\"\n            ></textarea>\n          </div>\n        <section className=\"editor\">\n\n\n          <Editor\n            apiKey={`${process.env.REACT_APP_TINY_API_KEY}`}\n            initialValue={query}\n            init={{\n              width: \"100vw\",\n              plugins: [\n                \"a11ychecker advcode advlist autolink link help imagetools image code lists charmap print preview hr anchor pagebreak\",\n                \" lists link linkchecker media mediaembed noneditable powerpaste preview\",\n                \"searchreplace wordcount visualblocks visualchars code fullscreen insertdatetime media nonbreaking\",\n                \"table emoticons template paste help\",\n              ],\n              a_plugin_option: true,\n              a_configuration_option: 400,\n              image_title: true,\n              automatic_uploads: true,\n              file_picker_types: \"image\",\n              toolbar:\n                \"insertfile undo redo a11ycheck | styleselect | bold italic | alignleft aligncenter alignright alignjustify | bullist numlist outdent indent | link image | print preview media fullpage | forecolor backcolor emoticons\",\n              menu: {\n                favs: {\n                  title: \"Shortcut\",\n                  items: \"code visualaid | searchreplace | emoticons\",\n                },\n              },\n              menubar: \"favs file edit view insert format tools table help\",\n              image_caption: true,\n              powerpaste_allow_local_images: true,\n            }}\n            onChange={handleEdtiorChange}\n          />\n        </section>\n        <Button type=\"submit\">Send</Button>\n      </form>\n    </section>\n  );\n};\n\nexport default Edit;\n","/Users/kadheryna/saro-react/src/pages/special/panel/AdminPanel.jsx",["338"],"import React, { useEffect } from \"react\";\nimport { useSelector } from \"react-redux\";\nimport { Tabs, Tab, Nav, Table, Button } from \"react-bootstrap\";\nimport { useContainer } from \"../../public/home/container\";\nimport { firestore } from \"../../../components/feature/firebase\";\n\nconst AdminPanel = () => {\n  const { getNews } = useContainer();\n  const newsItems = useSelector((state) => state.news.posts);\n\n  const removeItem = async (id) => {\n    return await firestore\n      .collection(\"language\")\n      .doc(\"en\")\n      .collection(\"news\")\n      .doc(id)\n      .delete();\n  };\n\n  useEffect(() => {\n    getNews();\n  }, []);\n\n  return (\n    <section className=\"section saro-panel\">\n      <h1>Saro CMS 1.0.0</h1>\n      <Tabs defaultActiveKey=\"newContent\" id=\"uncontrolled-tab-example\">\n        <Tab eventKey=\"newContent\" title=\"Add new content\">\n          <Nav className=\"flex-column\">\n            <Nav.Link href=\"/panel/add/news-content\">Add news</Nav.Link>\n            <Nav.Link href=\"/panel/add/post\">\n              Add new blog post\n            </Nav.Link>\n            <Nav.Link href=\"/panel/add/events\">\n              Add new event\n            </Nav.Link>\n            <Nav.Link href=\"/panel/add/article\">\n              Add new article\n            </Nav.Link>\n          </Nav>\n        </Tab>\n        <Tab eventKey=\"menagment\" title=\"Menage content\">\n          <Nav className=\"flex-row\" eventKey=\"news\">\n            <Nav.Link eventKey=\"news\" title=\"Edit news\">\n              Edit navgation\n            </Nav.Link>\n            <Nav.Link href=\"/panel/edit/nav\" disabled>\n              Edit navgation\n            </Nav.Link>\n            <Nav.Link href=\"/panel/edit/general\" disabled>\n              Edit general stuff\n            </Nav.Link>\n          </Nav>\n          <Table striped bordered hover>\n            <thead>\n              <tr>\n                <th>#</th>\n                <th>Title</th>\n                <th>Published date</th>\n                <th>Author</th>\n                <th>Menagement</th>\n              </tr>\n            </thead>\n            {newsItems.map((post, index) => {\n              const { author, title, date, id } = post;\n              return (\n                <tbody key={id}>\n                  <tr>\n                    <td>{index}</td>\n                    <td>{title}</td>\n                    <td>{date}</td>\n                    <td>{author}</td>\n                    <td>\n                      <Button variant=\"primary\" disabled>\n                        Edit\n                      </Button>\n                      <Button variant=\"danger\" onClick={() => removeItem(id)}>\n                        Remove\n                      </Button>\n                    </td>\n                  </tr>\n                </tbody>\n              );\n            })}\n          </Table>\n        </Tab>\n        <Tab eventKey=\"menagmentOfCrew\" title=\"Menage your profile\" disabled>\n          2\n        </Tab>\n        <Tab eventKey=\"menagmentOfPictrues\" title=\"Menage pictrues\" disabled>\n          3\n        </Tab>\n      </Tabs>\n    </section>\n  );\n};\n\nexport default AdminPanel;\n","/Users/kadheryna/saro-react/src/components/feature/auth/login/Login.jsx",["339"],"import React, { useEffect } from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport LoginForm from \"./LoginForm\";\nimport { auth } from \"../../firebase\";\nimport { userActions } from \"../../../../_actions\";\n\nconst Login = () => {\n  const dispatch = useDispatch();\n\n  const logout = async () => {\n    try {\n      dispatch(userActions.logout());\n      await auth.signOut();\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  useEffect(() => {\n    logout();\n  }, []);\n\n  return (\n    <section className=\"section login\">\n      <LoginForm />\n    </section>\n  );\n};\n\nexport default Login;\n","/Users/kadheryna/saro-react/src/components/feature/auth/signup/SignUp.jsx",[],"/Users/kadheryna/saro-react/src/_reducers/news.reducer.js",[],"/Users/kadheryna/saro-react/src/_reducers/hero.reducer.js",[],"/Users/kadheryna/saro-react/src/_reducers/auth.reducer.js",[],"/Users/kadheryna/saro-react/src/_reducers/nav.reducer.js",[],"/Users/kadheryna/saro-react/src/_reducers/blog.reducer.js",[],"/Users/kadheryna/saro-react/src/_reducers/general.reducer.js",[],"/Users/kadheryna/saro-react/src/_reducers/alerts.reducer.js",[],"/Users/kadheryna/saro-react/src/_reducers/events.reducer.js",[],"/Users/kadheryna/saro-react/src/_actions/index.js",[],"/Users/kadheryna/saro-react/src/components/feature/firebase/index.js",[],"/Users/kadheryna/saro-react/src/pages/public/blog/container.js",["340","341"],"import { fetchActions, blogActions, eventsActions } from \"../../../_actions\";\nimport { generalConstants } from \"../../../_constants\";\nimport { firestore } from \"../../../components/feature/firebase\";\nimport { useDispatch, useSelector } from \"react-redux\";\n\nexport const useContainer = () => {\n  const dispatch = useDispatch();\n  const lang = useSelector((state) => state.general.language);\n  const article = us