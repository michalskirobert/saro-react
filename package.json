{
  "name": "saro-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "@tinymce/tinymce-react": "^3.10.2",
    "bootstrap": "^4.6.0",
    "cra-template-pwa": "^1.0.3",
    "firebase": "^8.2.5",
    "formik": "^2.2.6",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "lodash.throttle": "^4.1.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "react-paginate": "^7.1.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-scroll-up": "^1.3.7",
    "react-select": "^4.3.0",
    "react-uuid": "^1.0.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sass": "^1.32.5",
    "styled-components": "^5.2.1",
    "web-vitals": "^0.2.4",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "del .eslintcache && react-scripts start",
    "build": "CI= react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.config.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn run test"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "npm run --silent lint:fix",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
